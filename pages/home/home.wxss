.container {
  background: #fff;
  height: 100%;
}

.banner {
  width: 750rpx;
  height: 417rpx;
}

.banner image {
  width: 100%;
  height: 417rpx;
}

.head {
  padding: 10rpx 0;
  display: flex;
}

.head view {
  flex: 1;
}

.head image {
  width: 28vw;
  height: 11vw;
}

.head .logo {
  margin-left: 5px;
}

.head .title_right {
  margin-right: 10px;
  width: 24vw;
}

.img {
  width: 40px;
  height: 40px;
  margin: 5px auto;
}

.circle {
  position: relative;
  height: 12.5vw;
  color: #fff;
  background: #f4f4f4;
  margin-bottom: 2%;
  overflow: hidden;
}

.circle>view {
  text-align: center;
  width: 25vw;
  height: 12.5vw;
  display: inline-block;
  border-radius: 0 0 12.5vw 12.5vw;
  opacity: 0.9;
  position: absolute;
  top: 0;
}

.circle>view image {
  display: block;
  width: 4vw;
  height: 4vw;
  margin: 0 auto;
  margin-top: 3px;
}

.circle .circle1 {
  background: #2963ad;
  left: -2vw;
}

.circle .circle2 {
  background: #fb27a0;
  left: 18vw;
}

.circle .circle3 {
  background: #a9da81;
  left: 38vw;
}

.circle .circle4 {
  background: #86cfef;
  left: 58vw;
}

.circle .circle5 {
  background: #fbf722;
  left: 78vw;
}

.productList image {
  width: 100%;
  display: block;
}

.productList .word {
  background: #ac2424;
}

.productList .word text {
  margin: 10px 0;
  color: #ac2424;
  background: #fff;
  padding: 5px 0;
  border-radius: 20px;
  display: inline-block;
  width: 56vw;
}

.imageTwo .word {
  background: #fa29a1;
}

.imageTwo .word text {
  color: #fa29a1;
}

.imageThree .word {
  background: #a9db82;
}

.imageThree .word text {
  color: #a9db82;
}

.break {
  background: #fff;
  height: 2vw;
  /* margin: 2% 0; */
}

.categoryName {
  padding: 2% 0;
  border-bottom: 2px solid #ececec;
  border-top: 2vw solid #ececec;
}

.categoryName text {
  display: inline-block;
  vertical-align: middle;
}

.categoryName .text {
  padding: 0 8px;
}

.categoryName .line {
  width: 40px;
  height: 1px;
  background: #bbb;
}

.b {
  width: 740rpx;
  padding: 0 5rpx;
  height: auto;
  overflow: hidden;
}

.b .item {
  float: left;
  background: #fff;
  width: 370rpx;
  margin-bottom: 6.25rpx;
  padding-bottom: 33.333rpx;
  height: auto;
  overflow: hidden;
  text-align: center;
  position: relative;
}

.b .item image {
  margin-left: 5rpx;
}

.b .addCart {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 30rpx;
  bottom: 20rpx;
}

.b .addCart  image {
  width: 100%;
  height: 100%;
}

.item .img {
  width: 360rpx;
  height: 360rpx;
}

.item .name {
  display: block;
  width: 320rpx;
  /* height: 40rpx; */
  margin: 11.5rpx 0;
  /* text-align: center; */
  overflow: hidden;
  /* padding: 0 20rpx; */
  font-size: 30rpx;
  color: #333;
}

.item .price {
  display: block;
  width: 365.625rpx;
  height: 30rpx;
  /* text-align: center; */
  font-size: 32rpx;
  /* color: #b4282d; */
}

.productList_one {
  border-bottom: 1vw solid #ececec;
}

/* xianshiqianggou */

.timeLimit {
  padding: 40rpx 4%;
  border-top: 2vw solid #ececec;
}

.timeLimit_tag {
  color: #ffe3e3;
  background: #ff707a;
  display: inline-block;
  font-style: italic;
  padding: 2px 10px;
  border-top-left-radius: 16px;
  border-bottom-right-radius: 16px;
  margin-left: 10px;
}

.timeLimit .timeLimit_list {
  padding-top: 4%;
}

.timeLimit .timeLimit_list .lineItem {
  -webkit-box-direction: normal;
  -webkit-box-orient: horizontal;
  -webkit-flex-direction: row;
  -moz-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: stretch;
  -webkit-align-items: stretch;
  -moz-align-items: stretch;
  align-items: stretch;
  margin-bottom: 60rpx;
  padding: 0;
}

.timeLimit .timeLimit_list .lineItem:last-child {
  margin-bottom: 0;
}

.timeLimit .timeLimit_list .lineItem .center {
  padding: 1% 0 1% 2%;
  -webkit-box-direction: normal;
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  -moz-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -moz-justify-content: space-between;
  justify-content: space-between;
}

.timeLimit .timeLimit_list .lineItem .left {
  width: 27vw;
  height: 27vw;
}

.timeLimit .timeLimit_list .lineItem .left image {
  border-radius: 8px;
}

.timeLimit_list .lineItem .center .tag {
  line-height: 1;
}

.timeLimit_list .lineItem .center .tag text {
  color: #ff9c9c;
  background: #ffeded;
  border-radius: 3px;
  line-height: 1.5;
  padding: 0 5px;
  margin-right: 5px;
}

/* .timeLimit_list .lineItem .center .price text{
  margin-right: 20px;
} */

.timeLimit_list .lineItem .center .price {
  position: relative;
}

.old_price {
  color: #999;
  text-decoration: line-through;
  margin-left: 20rpx;
}

.buy {
  padding: 2px 8px;
  border-radius: 30rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  margin-right: 0;
}

.time {
  position: absolute;
  bottom: 4em;
  right: 10rpx;
}

.time text {
  color: #999;
  background: #e6e6e6;
  width: 18px;
  height: 18px;
  line-height: 18px;
  border-radius: 50%;
  display: inline-block;
  margin: 0 2px;
}

.title {
  font-weight: 600;
}
