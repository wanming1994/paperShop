page {
  background: #efefef;
}

.container {
  padding: 2% 2% 0;
  text-align: center;
}

.head {
  background: #000;
  color: #fff;
  line-height: 1.6;
  padding-top: 5%;
}

.table {
  display: flex;
  background: #fff;
  margin-top: 5%;
  box-shadow: 5px 5px 5px #ddd;
  overflow: hidden;
  border-bottom: 1px solid #ddd;
  margin-bottom: 2vw;
}

.table>view {
  flex: 1;
}
.table>view view{
  height: 60rpx;
  line-height: 60rpx;
}
.table .iconImg{
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}

.member{
  border-bottom: 1px solid #efefef;
}
.member .left{
  width: 16vw;
  height: 16vw;
}
.member .left image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.member .center{
  margin-left: 3vw;
}